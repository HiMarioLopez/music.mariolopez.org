meta {
  name: Suggestions
  type: http
  seq: 1
}

get {
  url: https://music.mariolopez.org/api/nodejs/apple-music/catalog/us/search/suggestions?term=the+lumineers&limit=10&kinds=terms,topResults&types=songs,albums,artists
  body: none
  auth: bearer
}

params:query {
  term: the lumineers
  limit: 10
  kinds: terms,topResults
  types: songs,albums,artists
}

headers {
  Music-User-Token: {{MUSIC_USER_TOKEN}}
}

auth:bearer {
  token: {{AUTH_TOKEN}}
}
